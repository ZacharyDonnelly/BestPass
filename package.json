{
  "name": "bestpass-ui",
  "version": "1.0.0",
  "description": "Frontend UI",
  "author": "Zach Donnelly",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@reduxjs/toolkit": "1.9.7",
    "classnames": "2.5.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router": "6.27.0",
    "react-router-dom": "6.27.0"
  },
  "scripts": {
    "pre": "nvm use && source venv/bin/activate",
    "clean": "rimraf dist && rimraf node_modules",
    "test": "jest",
    "test:coverage": "jest --coverage --forceExit --colors",
    "lint:eslint": "eslint --fix --ignore-path .gitignore --ext .ts,.tsx",
    "lint:prettier": "prettier --write --loglevel warn \"**/*.{js,ts,json,scss,tsx}\"",
    "lint:stylelint": "stylelint \"**/*.{css,scss}\" --fix",
    "build": "rimraf dist && NODE_ENV=production BABEL_ENV=production rollup -c"
  },
  "devDependencies": {
    "@babel/core": "7.25.9",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.25.9",
    "@babel/preset-env": "7.25.9",
    "@babel/preset-react": "7.25.9",
    "@babel/preset-typescript": "7.25.9",
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-url": "7.0.0",
    "@svgr/rollup": "6.5.1",
    "@svgr/webpack": "6.5.1",
    "@testing-library/dom": "8.20.1",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "28.1.8",
    "@types/node": "18.19.59",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "babel-jest": "28.1.3",
    "babel-loader": "8.4.1",
    "babel-plugin-css-modules-transform": "1.6.2",
    "babel-plugin-named-asset-import": "0.3.8",
    "babel-preset-react-app": "10.0.1",
    "css-loader": "6.11.0",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-qunit": "7.3.0",
    "husky": "8.0.3",
    "jest": "28.1.3",
    "lint-staged": "13.3.0",
    "markdownlint-cli": "0.42.0",
    "postcss": "8.4.31",
    "postcss-scss": "4.0.9",
    "prettier": "2.8.8",
    "regenerator-runtime": "0.14.1",
    "rimraf": "3.0.2",
    "rollup": "2.75.7",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.36.0",
    "sass": "1.80.4",
    "sass-loader": "13.3.3",
    "scriptlint": "2.2.0",
    "style-loader": "3.3.3",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-prettier": "2.0.0",
    "stylelint-scss": "4.7.0",
    "typescript": "4.9.5",
    "url-loader": "4.1.1"
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "hooks": {
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "pre-commit": "lint-staged"
  },
  "scriptlint": {
    "rules": {
      "uses-allowed-namespace": false
    }
  }
}
