{
  "name": "bestpass-ui",
  "version": "1.0.0",
  "description": "Frontend UI",
  "author": "Zach Donnelly",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@reduxjs/toolkit": "1.8.6",
    "classnames": "2.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "6.3.0",
    "react-router-dom": "6.3.0"
  },
  "scripts": {
    "pre": "nvm use && source venv/bin/activate",
    "clean": "rimraf dist && rimraf node_modules",
    "test": "jest",
    "test:coverage": "jest --coverage --forceExit --colors",
    "lint:eslint": "eslint --fix --ignore-path .gitignore --ext .ts,.tsx",
    "lint:prettier": "prettier --write --loglevel warn \"**/*.{js,ts,json,scss,tsx}\"",
    "lint:stylelint": "stylelint \"**/*.{css,scss}\" --fix",
    "build": "rimraf dist && NODE_ENV=production BABEL_ENV=production rollup -c"
  },
  "devDependencies": {
    "@babel/core": "7.18.13",
    "@babel/plugin-proposal-class-properties": "7.17.12",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.18.10",
    "@babel/preset-env": "7.18.10",
    "@babel/preset-react": "7.17.12",
    "@babel/preset-typescript": "7.17.12",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-url": "7.0.0",
    "@svgr/rollup": "6.2.1",
    "@svgr/webpack": "6.2.1",
    "@testing-library/dom": "8.13.0",
    "@testing-library/jest-dom": "6.6.2",
    "@testing-library/react": "13.3.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.2.6",
    "@types/jest": "29.5.14",
    "@types/node": "18.0.0",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "babel-jest": "29.7.0",
    "babel-loader": "8.2.5",
    "babel-plugin-css-modules-transform": "1.6.2",
    "babel-plugin-named-asset-import": "0.3.8",
    "babel-preset-react-app": "10.0.1",
    "css-loader": "6.7.3",
    "eslint": "8.18.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-qunit": "7.3.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "lint-staged": "13.0.4",
    "markdownlint-cli": "0.31.1",
    "postcss": "8.4.31",
    "postcss-scss": "4.0.9",
    "prettier": "2.7.1",
    "regenerator-runtime": "0.13.11",
    "rimraf": "3.0.2",
    "rollup": "2.75.7",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.32.1",
    "sass": "1.52.3",
    "sass-loader": "13.0.2",
    "scriptlint": "2.2.0",
    "style-loader": "3.3.3",
    "stylelint": "14.9.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-prettier": "2.0.0",
    "stylelint-scss": "4.2.0",
    "typescript": "4.7.4",
    "url-loader": "4.1.1"
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "hooks": {
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "pre-commit": "lint-staged"
  },
  "scriptlint": {
    "rules": {
      "uses-allowed-namespace": false
    }
  }
}
